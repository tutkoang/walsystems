<% if flash[:alert] %>
	<%= js add_gritter(flash[:alert], title: "Tut Wal Portfolio", sticky: false) %>

<% elsif flash[:error] %>
	<%= js add_gritter(flash[:error], title: "Tut Wal Portfolio", sticky: false) %>
<% else %>
	<%= js add_gritter(flash[:notice], title: "Tut Wal Portfolio", sticky: false) %>
<% end %>